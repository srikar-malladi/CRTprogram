#include<stdio.h>
int main()
{
    int i;
    for(i=1;i<=20;i++)
    {
        printf("\n%d",i);
        if(i==11)
        exit(0);
    }
    printf("\n hello");
}
//*{
#include<stdio.h>
int main()
{
    int i,n;
    printf("\n enter a no to stop  ");
    scanf("\n ,&d");
    for(i=1;i<=20;i++)
    {
        printf("\n%d",i);
        if(i==n)
        break;
    }
    printf("\n hello");
}//