#include<stdio.h>
int main()
{
   int a,b,c ;
   printf("\n Enter 3 values : ");
   scanf("%d%d%d",&a,&b,&c);
   if(a==b && b==c)
     printf("\n all are equal");
   else if(a>b && a>c )
     printf("\n %d   is big",a);
    else if(b>c )
     printf("\n %d   is big",b);
    else
     printf("\n %d  is big ",c);
}