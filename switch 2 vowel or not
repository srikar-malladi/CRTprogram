#include<stdio.h>
main()
{
   char ch ;
   printf("\n Enter an alphabet : ");
   scanf("%c",&ch);
 switch(ch )
 {
     case 'a' : printf("\n is a vowel ");break;
     case 'e' : printf("\n is a vowel ");break;
     case 'i' : printf("\n is a vowel ");break;
     case 'o' : printf("\n is a vowel");break;
     case 'u' : printf("\n is a vowel");break;
     default: printf("\n is a consonent");
 }
 }
    